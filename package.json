      "typescript": "3.8.3"
   },
   "scripts": {
      "build:css": "postcss src/index.css -o src/styles/tailwind.css",
      "watch:css": "postcss src/index.css -o src/styles/tailwind.css",
      "start": "npm run watch:css & react-app-rewired start",
      "build": "npm run build:css & react-app-rewired build",
      "coverage": "react-app-rewired test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false",
      "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
      "eject": "react-app-rewired eject",
      "ts:start": "npm run watch:css & react-scripts start",
      "ts:build": "npm run build:css & react-scripts build",
      "ts:test": "react-scripts test --env=jest-environment-jsdom-sixteen",
      "ts:eject": "react-scripts eject",
      "deploy": "export PUBLIC_URL=/2019/$C9_USER/react && npm run build && ./deploy.sh",
      "storybook": "start-storybook -p 8080 -s public",
      "build-storybook": "build-storybook -s public",
      "pretty": "npx prettier --write \"src/**/*.js\""
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "./src/*.{js,jsx,ts,tsx}": [
         "npx prettier --write \"src/**/*.js\"",
         "eslint src/*.js --fix-dry-run"
      ]
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@babel/core": "7.7.2",
      "@babel/plugin-proposal-decorators": "7.7.0",
      "@babel/plugin-syntax-jsx": "7.2.0",
      "@babel/preset-env": "7.7.1",
      "@storybook/addon-actions": "5.3.18",
      "@storybook/addon-docs": "5.3.18",
      "@storybook/addon-knobs": "5.3.18",
      "@storybook/addon-links": "5.3.18",
      "@storybook/addon-storysource": "5.3.18",
      "@storybook/addons": "5.3.18",
      "@storybook/preset-create-react-app": "2.1.2",
      "@storybook/react": "5.3.18",
      "@testing-library/jest-dom": "5.5.0",
      "@testing-library/react": "10.0.3",
      "@testing-library/react-native": "5.0.3",
      "babel-eslint": "10.0.3",
      "customize-cra": "0.9.1",
      "husky": "4.2.5",
      "jest-environment-jsdom-sixteen": "1.0.3",
      "lint-staged": "10.2.6",
      "react-app-rewired": "2.1.5",
      "tailwindcss": "^1.2.0"
   },
   "jest": {
      "testEnvironment": "node",
      "collectCoverageFrom": [
         "!src/authentication/utils/**/*.{ts,tsx,js,jsx}",
         "!src/authentication/routes/**/*.{ts,tsx,js,jsx}",
         "!src/App.tsx",
         "!src/common/components/HomePage/**/*.{ts,tsx,js,jsx}",
         "!src/authentication/services/**/*.{ts,tsx,js,jsx}",
         "!src/common/components/NoDataView/**/*.{ts,tsx,js,jsx}",
         "!src/common/components/Page1/**/*.{ts,tsx,js,jsx}",
         "!src/projectManagementPlatformApp/routes/**/*.{ts,tsx,js,jsx}",
         "!src/projectManagementPlatformApp/services/**/*.{ts,tsx,js,jsx}",
         "!src/projectManagementPlatformApp/stores/**/*.{ts,tsx,js,jsx}",
         "!src/projectManagementPlatformApp/components/adminComponents/CreateWorkflow/**/*.{ts,tsx,js,jsx}",
         "!src/index.js",
         "!src/serviceWorker.ts",
         "!src/common/components/LoadingWrapperWithFailure/**/*.{ts,tsx,js,jsx}",
         "!src/projectManagementPlatformApp/utils/**/*.{ts,tsx,js,jsx}",
         "!src/Authentication/services/AuthService/**/*.{ts,tsx,js,jsx}",
         "!src/index.tsx",
         "!src/Common/utils/APIUtils.js",
         "!src/Authentication/routes/authenticationRoutes.tsx",
         "!src/Authentication/stores/index.ts",
         "!src/ProjectManagementPlatformApp/services/endPoints.ts",
         "!src/ProjectManagementPlatformApp/stores/index.ts",
         "!src/ProjectManagementPlatformApp/routes/projectManagementPlatformRoutes.tsx",
         "!src/ProjectManagementPlatformApp/services/TaskService/index.Api.ts",
         "!src/ProjectManagementPlatformApp/services/ProjectsService/index.Api.ts",
         "!src/Common/components/CommonButton/CommonButton.stories.js",
         "!src/Common/components/LoadingWrapperWithFailure/FailureView.stories.js",
         "!src/Common/components/NoDataView/NoDataView.stories.js",
         "!src/Common/components/UserTextInputField/UserTextInputField.stories.js",
