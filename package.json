      "@types/react-dom": "16.9.6",
      "apisauce": "1.1.1",
      "autoprefixer": "^9.7.5",
      "babel-plugin-macros": "^2.8.0",
      "js-cookie": "2.2.1",
      "mobx": "5.15.0",
      "mobx-react": "6.1.4",
      "postcss-cli": "^7.1.0",
      "query-string": "6.13.0",
      "react": "^16.13.0",
      "react-cookie-consent": "5.0.1",
      "react-dom": "^16.13.0",
      "react-dropdown": "1.7.0",
